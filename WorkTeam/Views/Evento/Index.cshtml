@model IEnumerable<WorkTeam.Models.Evento>

@{
    ViewBag.Title = "Index";
}

<h2>Agenda de Eventos</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.evento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hora)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.evento)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Hora)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Fecha)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Fecha }) |
        @Html.ActionLink("Details", "Details", new { id = item.Fecha }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Fecha })
    </td>
</tr>
}

</table>
